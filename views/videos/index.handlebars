<p>INDEX PAGE</p>
<div class="contents-container" id="videos-container">
  {{#each videos as |video|}}
    <div class="video">
      <span class="title">{{ video.title }}</span>
      <iframe class="video-player" src="http://www.youtube.com" frameborder="0" allowfullscreen></iframe>
      <span class="description">{{ video.description }}</span>
    </div>
  {{/each}}
</div>
<a href="videos/create" class="add-video">Add Video</a>
